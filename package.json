{
  "name": "project-management-app",
  "version": "1.0.0",
  "description": "Run app client and server",
  "main": "index.js",
  "scripts": {
    "install-all": "npm i && cd client && npm i && cd ../server && npm i",
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "server": "npm run dev",
    "client": "npm start --prefix client",
    "start-app": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "mongoose": "^8.4.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "nodemon": "^2.0.20"
  }
}
